{
  "name": "magic-cut-image-editor",
  "version": "1.0.1",
  "description": "An image editor library for magic-cut app (http://www.magic-cut.in/)",
  "author": {
    "email": "b.lasledj@gmail.com",
    "name": "Billel LASLEDJ"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bil0/magic-cut-image-editor"
  },
  "keywords": [
    "Typescript",
    "Angular",
    "Image",
    "edit",
    "crop",
    "svg"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build mc-image-editor --prod && npm run copyfiles",
    "build:demo": "ng build image-editor-demo --prod",
    "copyfiles": "copyfiles ./CHANGELOG.md LICENSE ./dist/mc-image-editor/",
    "format:check": "prettier --list-different projects/**/*.ts",
    "format:write": "prettier --write projects/**/*.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "scripts": {
      "postbump": "node scripts/postbump",
      "postcommit": "node scripts/postcommit"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "lodash": "^4.17.20",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt": "^9.1.2",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.2.13",
    "ng-packagr": "^10.0.0",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "semantic-release": "^17.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.7"
  }
}
